<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.admin.mapper.BannerMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.admiin.pojo.Banner">
        <id column="banner_id" property="bannerId" />
        <result column="image" property="image" />
        <result column="type" property="type" />
        <result column="on" property="on" />
        <result column="text" property="text" />
        <result column="content_id" property="contentId" />
        <result column="sort" property="sort" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        banner_id, image, type, on, text, content_id, sort
    </sql>

</mapper>
