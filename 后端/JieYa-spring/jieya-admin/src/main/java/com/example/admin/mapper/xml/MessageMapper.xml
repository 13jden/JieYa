<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.admin.mapper.MessageMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.common.pojo.Message">
        <id column="id" property="id" />
        <result column="user" property="user" />
        <result column="to_user" property="toUser" />
        <result column="content" property="content" />
        <result column="type" property="type" />
        <result column="time" property="time" />
        <result column="FileUrl" property="FileUrl" />
        <result column="status" property="status"></result>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user, to_user, content, type, time, FileUrl,status;
    </sql>

</mapper>
