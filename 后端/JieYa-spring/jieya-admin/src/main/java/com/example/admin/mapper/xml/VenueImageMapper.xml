<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.admin.mapper.VenueImageMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.common.pojo.VenueImage">
        <id column="id" property="id" />
        <result column="venue_id" property="venueId" />
        <result column="image_url" property="imageUrl" />
        <result column="is_detail" property="isDetail" />
        <result column="sort_order" property="sortOrder" />
        <result column="created_at" property="createdAt" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, venue_id, image_url, is_detail, sort_order, created_at
    </sql>

</mapper>
